# {{ ansible_managed }}

<VirtualHost {{ apache_host|default('*') }}:{{ apache_port }}>
  ServerName                            {{ webhost }}{{ item }}.{{ webdomain }}
{% for alias in app_env.aliases|default([]) %}
  ServerAlias                           {{ alias }}
{% endfor %}

  Include {{ app_name }}{{ item }}-common.conf
</VirtualHost>
